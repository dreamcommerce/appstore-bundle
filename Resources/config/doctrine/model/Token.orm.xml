<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <mapped-superclass name="DreamCommerce\Bundle\ShopAppstoreBundle\Model\Token">
        <field name="expiresAt" type="datetime" column="expires_at" nullable="true"/>
        <field name="accessToken" type="string" column="access_token" length="50" nullable="true"/>
        <field name="refreshToken" type="string" column="refresh_token" length="50" nullable="true"/>
    </mapped-superclass>
</doctrine-mapping>
