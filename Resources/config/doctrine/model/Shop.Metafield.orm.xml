<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity
          name="DreamCommerce\Bundle\ShopAppstoreBundle\Model\Shop\Metafield"
          table="metafields"
  >

    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>


    <field name="metafieldExternalId" type="integer" column="metafield_external_id"/>
    <field name="metafieldKey" type="string" column="metafield_key" length="40"/>
    <field name="namespace" type="string" column="namespace" length="40"/>
    <field name="description" type="string" column="description" length="255"/>
    <field name="object" type="string" nullable="true" column="object" length="255"/>


    <one-to-many field="metafieldValues" target-entity="DreamCommerce\Bundle\ShopAppstoreBundle\Model\Shop\MetafieldValue" mapped-by="metafield">
    </one-to-many>
    <many-to-one target-entity="DreamCommerce\Bundle\ShopAppstoreBundle\Model\ShopInterface" field="shop">
      <join-column name="shop_id" referenced-column-name="id"/>
    </many-to-one>

  </entity>
</doctrine-mapping>